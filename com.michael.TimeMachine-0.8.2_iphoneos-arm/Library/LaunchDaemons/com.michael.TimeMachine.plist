<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.michael.TimeMachine</string>
	<key>Nice</key>
	<integer>-5</integer>
	<key>Program</key>
	<string>/usr/libexec/TimeMachine</string>
	<key>StandardErrorPath</key>
	<string>/var/log/TimeMachine-Error.log</string>
	<key>StandardOutPath</key>
	<string>/var/log/TimeMachine.log</string>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>2</integer>
		<key>Minute</key>
		<integer>0</integer>
	</dict>
	<key>UserName</key>
	<string>root</string>
</dict>
</plist>
